{
  "comments": "This disables pyright type checking for the entire project. This project is using mypy.",
  "exclude": [
    ".git",
    ".eggs",
    ".github",
    ".history",
    ".idea",
    ".tox",
    ".vscode",
    "docs",
    "**/__pycache__",
    "LifeWorks_procaliper.egg-info",
    "**/*.tsv",
    "**/.venv/**",
    "**/*.pdb"
  ],
  "ignore": [
    "src/oldstuff"
  ],
  "typeCheckingMode": "off"
}